#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x555b3139f100 .scope module, "tb" "tb" 2 1;
 .timescale 0 0;
v0x555b313e3db0_0 .var "clk", 0 0;
v0x555b313e3e50_0 .net "out", 2 0, v0x555b313e3c70_0;  1 drivers
S_0x555b3139f280 .scope module, "dut0" "dut" 2 13, 2 21 0, S_0x555b3139f100;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /OUTPUT 3 "out"
v0x555b313d5760_0 .net "clk", 0 0, v0x555b313e3db0_0;  1 drivers
v0x555b313e3c70_0 .var "out", 2 0;
E_0x555b313d5cf0 .event posedge, v0x555b313d5760_0;
    .scope S_0x555b3139f280;
T_0 ;
    %wait E_0x555b313d5cf0;
    %load/vec4 v0x555b313e3c70_0;
    %addi 1, 0, 3;
    %assign/vec4 v0x555b313e3c70_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x555b3139f100;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x555b313e3db0_0, 0, 1;
T_1.0 ;
    %delay 5, 0;
    %load/vec4 v0x555b313e3db0_0;
    %inv;
    %store/vec4 v0x555b313e3db0_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x555b3139f100;
T_2 ;
    %vpi_call 2 15 "$dumpfile", "exp20.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x555b3139f100 {0 0 0};
    %delay 1000, 0;
    %vpi_call 2 17 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "exp20.v";
